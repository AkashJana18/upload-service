{
  "name": "@w3ui/react",
  "version": "2.5.5",
  "description": "React adapter for w3ui.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "bunchee --watch",
    "build": "bunchee",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --build",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}' && prettier --check '**/*.{js,jsx,ts,tsx,yml,json}' --ignore-path ../../../../.gitignore",
    "lint:fix": "eslint --fix '**/*.{js,jsx,ts,tsx}' && prettier --write '**/*.{js,jsx,ts,tsx,yml,json}' --ignore-path ../../../../.gitignore",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "files": [
    "dist/*",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web3-storage/w3ui.git"
  },
  "author": "Alan Shaw",
  "license": "Apache-2.0 OR MIT",
  "bugs": {
    "url": "https://github.com/web3-storage/w3ui/issues"
  },
  "homepage": "https://github.com/web3-storage/w3ui/tree/main/packages/react",
  "dependencies": {
    "@ariakit/react": "catalog:",
    "@ariakit/react-core": "catalog:",
    "@w3ui/core": "workspace:^",
    "ariakit-react-utils": "catalog:"
  },
  "devDependencies": {
    "@ipld/dag-ucan": "^3.2.0",
    "@testing-library/react": "catalog:",
    "@testing-library/user-event": "catalog:",
    "@types/react": "catalog:",
    "@ucanto/client": "catalog:",
    "@ucanto/interface": "^10.0.1",
    "@ucanto/principal": "^9.0.1",
    "@ucanto/transport": "catalog:",
    "bunchee": "catalog:",
    "eslint-plugin-react-hooks": "catalog:",
    "fake-indexeddb": "catalog:",
    "happy-dom": "catalog:",
    "jsdom": "catalog:",
    "multiformats": "^13.1.0",
    "react": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "eslintConfig": {
    "extends": [
      "../../eslint.packages.js",
      "plugin:react-hooks/recommended"
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "dist"
  ]
}
